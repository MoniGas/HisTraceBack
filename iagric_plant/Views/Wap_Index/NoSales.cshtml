@model LinqModel.CodeInfo
@{
    Layout = null;
}
<!doctype html>
<html>
<head>
    <meta charset="utf-8">
    <title>医疗器械（UDI）服务云平台</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0,user-scalable=no"
        id="viewport">
    <link href="/css/home.min.css" rel="stylesheet" type="text/css" />
</head>
<body>
    <section class="section-module">
    <section> 
    <dl class="contact-dl" style="width:100%;">
<dd style="color:Red;width:100%; text-align:center; font-weight:bold">该码未激活：二维码码内容。</dd>
</dl>
@if (Model != null)
{
    if (null != Model.FwCode)
    {
        if (!string.IsNullOrEmpty(Model.FwCode.EWM))
        {
            <dl class="contact-dl" style="word-break: break-all;">
                <dt style="width: 45px;">码值：</dt>
                <dd>@Model.FwCode.EWM</dd>
            </dl>
        }
    }
}
</section>
</section>
</body>
</html>
