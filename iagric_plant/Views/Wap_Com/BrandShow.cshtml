@model LinqModel.Brand
@{
    ViewBag.Title = "BrandShow";
    Layout = null;
}

<!doctype html>
<html>
<head>
    <meta charset="utf-8">
    <title>医疗器械（UDI）服务云平台</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0,user-scalable=no"
        id="viewport">
    <link href="/css/home.min.css" rel="stylesheet" type="text/css" />
    <script src="/Scripts/jquery-1.7.2.min.js" type="text/javascript"></script>
    <script type="text/javascript">
        $(function () {
            $("#divFooter").height(35);
            $("#footer").height(35);

            if ($(window).height() > document.body.clientHeight) {
                $("#divBody").height($(window).height() - $("#divFooter").height());
            }
        });
    </script>
</head>
<body>
<div id="divBody">
    <!-- 页头 -->
    <section>
	    <header class="headerbox">
	    	<a href="javascript:history.go(-1);" class="art-back">返回</a>
		    <h1 title="医疗器械（UDI）服务云平台">@Model.BrandName</h1>
	    </header>
    </section>
    <!--联系-->
    <section class="section-module">
    <div class="book-detail-top clearfix">
        <div class="img">
            <img src="@Model.Logo" width="90"/>
        </div>
        <div class="info">
            <h3>
                @Model.BrandName
            </h3>
            <p>
                @Model.Descriptions
            </p>
        </div>
        </div>
    </section>
    </div>
    <div id="divFooter" class="juchou-footer">
        <footer id="footer">
        <p>医疗器械（UDI）服务云平台</p>
        </footer>
    </div>
</body>
</html>

